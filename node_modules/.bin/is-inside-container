#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/p/workshop/WEBSITE/thomid.me/node_modules/is-inside-container/node_modules:/mnt/p/workshop/WEBSITE/thomid.me/node_modules:/mnt/p/workshop/WEBSITE/node_modules:/mnt/p/workshop/node_modules:/mnt/p/node_modules:/mnt/p/workshop/WEBSITE/thomid.me/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/p/workshop/WEBSITE/thomid.me/node_modules/is-inside-container/node_modules:/mnt/p/workshop/WEBSITE/thomid.me/node_modules:/mnt/p/workshop/WEBSITE/node_modules:/mnt/p/workshop/node_modules:/mnt/p/node_modules:/mnt/p/workshop/WEBSITE/thomid.me/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-inside-container/cli.js" "$@"
else
  exec node  "$basedir/../is-inside-container/cli.js" "$@"
fi
